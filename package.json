{
  "name": "whatsapp-archive",
  "version": "1.0.0",
  "description": "A wrapper around WhatsApp Web which archives all messages to your computer",
  "repository": "https://github.com/twpol/whatsapp-archive.git",
  "author": "James Ross <whatsapp-archive@james-ross.co.uk>",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder --publish=onTag",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "devDependencies": {
    "electron": "^22.3.21",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2"
  },
  "dependencies": {
    "source-map-support": "^0.5.13"
  }
}
